<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.protectionapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.TRANSMIT_IR"
        android:required="false" />

    <uses-feature
        android:name="android.hardware.consumerir"
        android:required="false" />

    <permission
        android:name="android.permission.CAPTURE_AUDIO_OUTPUT"
        android:protectionLevel="signature|privileged" />

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.ANSWER_PHONE_CALLS" />
    <uses-permission android:name="android.permission.PROCESS_INCOMING_CALLS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission
        android:name="android.permission.CLEAR_APP_CACHE"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />

    <uses-feature
        android:name="android.hardware.touchscreen"
        android:required="false" />

    <application
        android:name="org.mazhuang.cleanexpert.Protection"
        android:allowBackup="true"
        android:icon="@mipmap/protectionapp"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/protectionapp_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Base.Light"
        android:usesCleartextTraffic="true"
        tools:replace="android:allowBackup,android:icon,android:name,android:theme">


        <activity android:name=".RecordsActivites.PassportFile"></activity>
        <activity android:name=".RecordsActivites.Passport" />

        <activity android:name=".activites.IRCameraDetector"></activity>
        >>>>>>> Stashed changes
        <activity android:name=".RecordsActivites.StudentIDFile" />
        <activity android:name=".RecordsActivites.VoterIDFile" />
        <activity android:name=".RecordsActivites.ATMFile" />
        <activity android:name=".RecordsActivites.BankFile" />
        <activity android:name=".RecordsActivites.DrivingLicenseFile" />
        <activity android:name=".RecordsActivites.StudentID" />
        <activity android:name=".RecordsActivites.PanFile" />
        <activity android:name=".RecordsActivites.AdhaarFile" />
        <activity android:name=".RecordsActivites.DocumentList" />
        <activity android:name=".cameraDetector.OutsideTips" />
        <activity android:name=".cameraDetector.Changingroom" />
        <activity android:name=".cameraDetector.BathroomTips" />
        <activity android:name=".cameraDetector.BedroomTips" />
        <activity android:name=".cameraDetector.CamDetector_tips" />
        <activity android:name=".cameraDetector.CameraDectectorByRM" />
        <activity android:name=".activites.Payment_premiumUser" />
        <activity android:name=".activites.KillNotification" />
        <activity android:name=".activites.Otp" />
        <activity android:name=".RecordsActivites.VoterID" />
        <activity android:name=".RecordsActivites.ATM" />
        <activity android:name=".RecordsActivites.Bank" />
        <activity android:name=".RecordsActivites.DrivingLicence" />
        <activity android:name=".RecordsActivites.PAN" />
        <activity android:name=".RecordsActivites.Adhaar" />
        <activity android:name=".activites.CameraDetector" />
        <activity android:name=".activites.FileShare" />
        <activity android:name=".activites.CacheCleaner" />
        <activity android:name=".activites.CallRecorder" />
        <activity android:name=".activites.HomePage" />
        <activity android:name=".activites.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.LogIn"
            android:label="LogIn">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="protectionapp.com"
                    android:scheme="https" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/filepaths" />
        </provider>

        <meta-data
            android:name="com.google.android.gms.wallet.api.enabled"
            android:value="true" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/login_logo" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

        <service android:name=".services.RecordingService" /> <!-- <service android:name=".hoverwindow.HoverService" /> -->
        <service
            android:name=".services.ForgroundService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".services.BlockListener"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService" />
            </intent-filter>
        </service>
        <service
            android:name=".services.FloatingWindowService"
            android:enabled="true"
            android:exported="false" />
        <service android:name=".services.NotificationsMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <receiver android:name=".receivers.BootCompleteReceiver">
            <intent-filter android:priority="999">
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.QUICKBOOT_POWERON" />
                <action android:name="com.htc.intent.action.QUICKBOOT_POWERON" />
            </intent-filter>
        </receiver>

        <receiver android:name=".receivers.CallReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>

        </receiver>

        <receiver
            android:name=".receivers.ControlRecordingReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="net.synapticweb.callrecorder.START_RECORDING" />
                <action android:name="net.synapticweb.callrecorder.STOP_SPEAKER" />
                <action android:name="net.synapticweb.callrecorder.START_SPEAKER" />
            </intent-filter>
        </receiver>

        <service android:name=".services.CallRecorderService" />
        <service
            android:name=".services.AccessibilityKeyDetector"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>
            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accesiblity_service" />

        </service>
    </application>

</manifest>