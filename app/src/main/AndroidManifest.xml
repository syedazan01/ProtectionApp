<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.protectionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/protectionapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/protectionapp_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:theme="@style/AppTheme.NoActionBar"
            android:name=".CameraDetector"></activity>
        <activity android:name=".FileShare" />
        <activity android:name=".CacheCleaner" />
        <activity android:name=".CallRecorder" />
        <activity android:name=".activites.HomePage" />
        <activity android:name=".activites.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.LogIn"
            android:label="LogIn" />
    </application>

</manifest>